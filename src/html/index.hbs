{{> _partials/header}}
<div id="wrapper">
  {{> _partials/nav}}
    <div id="page-wrapper">
      <div class="row">
          <div class="col-lg-12">
              <h3 class="page-header">Strato Data Dashboard</h3>
          </div>
          <!-- /.col-lg-12 -->
      </div>
      <!-- /.row -->
      <div class="row">
          <div class="col-lg-3 col-md-6">
              {{> _partials/dashboard/stats/daily-report-count}}
          </div>

          <div class="col-lg-3 col-md-6">
              {{> _partials/dashboard/stats/status-ok}}
          </div>
          
          <div class="col-lg-3 col-md-6">
              {{> _partials/dashboard/stats/status-failed}}
          </div>
          <div class="col-lg-3 col-md-6">
              {{> _partials/dashboard/stats/status-unknown}}
          </div>
      </div>
      <!-- /.row -->
      <div class="row">
          <div class="col-lg-4 col-lg-push-8">
              {{> _partials/dashboard/functionality-chart}}
              {{> _partials/dashboard/recent-reports}}
          </div>
          <div class="col-lg-8 col-lg-pull-4">
          		{{> _partials/dashboard/map}}
              {{> _partials/dashboard/aggregates-chart}}
          </div>
          <!-- /.col-lg-8 -->
          
          <!-- /.col-lg-4 -->
      </div>
      <!-- /.row -->
    </div>
    <!-- /#page-wrapper -->
</div>

{{> _partials/commonjs}}
<!-- Morris Charts JavaScript -->
<script src="bower_components/raphael/raphael-min.js"></script>
<script src="bower_components/morrisjs/morris.min.js"></script>

<script src="js/countries.geo.js" type="text/javascript"></script>
<script src="bower_components/leaflet/dist/leaflet.js"></script>
<script src="bower_components/leaflet.markercluster/dist/leaflet.markercluster.js"></script>

<script src="js/dashboard.js"></script>

